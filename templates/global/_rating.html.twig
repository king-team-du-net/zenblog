<ul class="list-inline mb-0 {% if class is defined %}{{ class }}{% endif %}">
    {% for i in 1..5 %}
        {% if i <= rating %}
            <li class="list-inline-item small me-0"><i class="fas fa-star text-warning"></i></li>
        {% else %}
            <li class="list-inline-item small me-0"><i class="far fa-star text-warning"></i></li>
        {% endif %}
    {% endfor %}
</ul>
