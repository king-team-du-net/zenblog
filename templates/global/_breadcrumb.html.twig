<section class="pb-4">
	<div class="container">
		<div class="row">
            <div class="col-12">
				<div class="bg-primary bg-opacity-10 p-4 p-md-5 rounded-3 text-center">
					<h1 class="text-primary">{{ breadcrumb|last['current'] }}</h1>
					<nav class="d-flex justify-content-center" aria-label="breadcrumb">
						<ol class="breadcrumb breadcrumb-dots m-0">
							<li class="breadcrumb-item"><a href="{{ path('homepage') }}"><i class="bi bi-house me-1"></i> {{'label.breadcrumb-home'|trans}}</a></li>
                            {% for li in breadcrumb%}
                                {% for routename, pagetitle in li %}
                                    {% if routename != "current" %}
                                        {% if '/' in routename %}
                                            {% set href = routename %}
                                        {% else %}
                                            {% set href = path(routename) %}
                                        {% endif %}
                                        <li class="breadcrumb-item"><a href="{{ href }}" class="">{{pagetitle}}</a></li>
                                    {% endif %}
                                {% endfor %}
                            {% endfor %}
                            <li class="breadcrumb-item active">{{ breadcrumb|last['current'] }}</li>
						</ol>
					</nav>
				</div>
            </div>
        </div>
	</div>
</section>
<!-- =======================
Inner intro END -->