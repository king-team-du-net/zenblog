{% if route is not null %}
    <li>
        <a class="{{ this.active ? 'active' }}" href="{{ path(route) }}" {{ attributes }}>
            {{ label|trans }}
        </a>
    </li>
{% else %}
    <li>
        <a class="" href="#" {{ attributes }}>
            {{ label|trans }}
        </a>
    </li>
{% endif %}
