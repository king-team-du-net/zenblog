{% extends "bundles/TwigBundle/Exception/error.html.twig" %}

{% set pagetitle = 'http_error.name'|trans({ 'status_code': 404 }) %}
{% block title pagetitle %}
{% block body_id 'error' %}

{% block body %}
    <h1 class="display-1 text-primary">{{ pagetitle }}</h1>
    <h2 class="fs-lg pb-2 pb-md-0 mb-4 mb-md-5">{{ 'http_error_404.description'|trans }}</h2>
    <p>{{ 'http_error_404.suggestion'|trans({ 'url': path('homepage') })|raw }}</p>
{% endblock %}
